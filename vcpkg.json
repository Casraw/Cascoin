{
  "name": "cascoin-core",
  "version": "2.0.5",
  "description": "Cascoin Core - Cryptocurrency full node implementation",
  "dependencies": [
    {
      "name": "boost",
      "features": [
        "system",
        "filesystem",
        "program-options",
        "thread",
        "chrono"
      ]
    },
    "openssl",
    "libevent",
    "protobuf",
    {
      "name": "zeromq",
      "platform": "!uwp"
    },
    {
      "name": "qrencode",
      "platform": "!uwp"
    },
    {
      "name": "miniupnpc",
      "platform": "!uwp"
    },
    {
      "name": "berkeleydb",
      "version>=": "4.8.0"
    },
    {
      "name": "qt6",
      "default-features": false,
      "features": [
        "base",
        "tools",
        "svg",
        "widgets",
        "network"
      ]
    }
  ],
  "builtin-baseline": "2024-01-01",
  "overrides": [
    {
      "name": "berkeleydb",
      "version": "4.8.30"
    }
  ]
}
